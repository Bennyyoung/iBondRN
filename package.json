{
  "name": "BondMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:release": "react-native run-android --mode release",
    "ios": "react-native run-ios",
    "ios-15": "react-native run-ios --simulator='iPhone 15 Pro'",
    "ios:release": "react-native run-ios --configuration Release",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "bundle:ios": "npx react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "clean-a": "watchman watch-del-all && cd android && chmod +x gradlew && ./gradlew clean && cd ..",
    "assemble": "cd android && ./gradlew assembleRelease && cd ..",
    "bundle:android": "npx react-native bundle --dev false --platform android  --entry-file index.js --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res",
    "preassemble": "cd android && ./gradlew clean",
    "pod-install": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install && cd .."
  },
